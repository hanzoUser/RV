                        else if tool == 'power.via.avg.worstcase_normal'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:176px') pwr.via.AVG_dataTR0.5.WC
                        else if tool == 'power.via.avg.worstcase_dataTR0.2'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:176px') pwr.via.AVG_dataTR0.2.WC
                        else if tool == 'power.metal.peak.worstcase_normal'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:202px') pwr.metal.PEAK_dataTR0.5.WC
                        else if tool == 'power.metal.peak.worstcase_dataTR0.2'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:202px') pwr.metal.PEAK_dataTR0.2.WC
                        else if tool == 'ward'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:545px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'ward_normal'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:545px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'ward_ind'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:545px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'latest_dbtag'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:285px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'ir_dbtag'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:285px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'em_dbtag'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:285px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'latest_checkin_date'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:200px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'sh'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:156px',onclick="sortTable("+table1count+")") Self-heat >20
                        else if tool == 'sf'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:156px',onclick="sortTable("+table1count+")") I/Idr => 2
                        else if tool == '260rescheck'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:156px',onclick="sortTable("+table1count+")") 260rescheck
                        else if tool == '400rescheck'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:156px',onclick="sortTable("+table1count+")") 400rescheck
                        else if tool == 'rescheck_worstcase'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:93px') rescheck_worstcase
                        else if tool == 'check_hip_pg'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:115px') check_hip_pg
                        else if tool == 'check_secondary_pg_cells'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:142px') check_secondary_pg_cells
                        else if tool == 'apl_cap_check'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:145px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'apl_current_check'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:145px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'ccs_timing_check'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:145px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'lef_check'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:105px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'lib_check'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:105px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'lib_nldm'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:105px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'spef_check'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:105px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'timing_window'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:135px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'timing_slew'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:135px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'w2w.metal_dataTR0.5'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:168px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'w2w.metal_dataTR0.5.WC'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:168px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'w2w.via_dataTR0.5'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:151px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'w2w.via_dataTR0.5.WC'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:151px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'w2w.metal_dataTR0.2'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:168px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'w2w.metal_dataTR0.2.WC'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:168px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'w2w.via_dataTR0.2'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:151px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'w2w.via_dataTR0.2.WC'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:151px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'vdm_nom_Reff'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:99px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'vdm_hv_Reff'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:109px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'odi_nom_Reff'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:115px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'pg_temp_worstcase'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:98px',onclick="sortTable("+table1count+")") #{tool}
                        else
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:105px') #{tool}

            tbody
                for key, part in testing
                    tr
                        td #{part}
    
                        for tool in Tools
                            - var val 
                            - if(key[tool] < 0){
                            -    val = 0
                            - }
                            - else{
                            -    val = key[tool]
                            - }
                            if tool.indexOf('drcdf_base1') != -1 & val > 0
                                td
                                    a(href="/rvarlcj0violations/loadviolation/?partition="+part+"&viol_type="+"drcdf_base1", data-toogle="tooltip", title=part + " " + tool) #{val}
                            else if tool.indexOf('drcdf_base2') != -1 & val > 0
                                td
                                    a(href="/rvarlcj0violations/loadviolation/?partition="+part+"&viol_type="+"drcdf_base2", data-toogle="tooltip", title=part + " " + tool) #{val}
                            else if tool.indexOf("complete_date") != -1
                                td #{val}
                            else if tool.indexOf("ward") != -1
                                td #{val}
                            else if tool.indexOf("nlib_source") != -1
                                td #{val}
                            else if tool.indexOf("dbtag") != -1
                                td #{val}
                            else if tool.indexOf("checkin_date") != -1
                                td #{val}
                            else if tool.indexOf("Gated") != -1 & val > 0
                                td
                                    a(href="/rvarlcj0violations/loadviolation/?partition="+part+"&viol_type="+"gdvd", data-toogle="tooltip", title="View violations") #{val}
                            else if tool.indexOf("Ungated") != -1 & val > 0
                                td
                                    a(href="/rvarlcj0violations/loadviolation/?partition="+part+"&viol_type="+"udvd", data-toogle="tooltip", title="View violations") #{val}
                            else if tool.indexOf('worstcase') != -1 
                                td #{val}
                            else if tool.indexOf('.WC') != -1 
                                td #{val}
                            else if tool == 'missing AON VL'
                                pass
                            else if tool == 'missing AON v4'
                                pass
  
                            else if tool == 'HIP No Power'
                                pass
                            else if tool == 'reused'
                                td #{val}
                            else if tool == 'fit_value'
                                td #{val}
                            else if tool == 'ir_md_def'
                                td #{val}
                            else if tool == 'em_md_def'
                                td #{val}

                            else if tool == 'HIP <50% vias'
                                pass 

                            else if tool == 'unbounded AON'
                                pass

                            else if tool == 'glbdrv v6 missing'
                                pass
                            else if tool == 'glbdrv v5 missing'
                                pass
                            else
                                td 
                                    a(href="/rvarlcj0violations/loadviolation/?partition="+part+"&viol_type="+tool, data-toogle="tooltip", title=part + " " + tool) #{val}

    script(type='text/javascript', src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js")
    script(type="text/javascript", src="https://milankyncl.github.io/jquery-copy-to-clipboard/jquery.copy-to-clipboard.js")
    script(src="searchfilterrv.js")
    script(src='copytext.js')
    script(src="../loadviolations.js")
    script.
        $('[data-toggle="tooltip"]').tooltip(); 


