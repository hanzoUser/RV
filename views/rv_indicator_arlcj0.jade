                        else if tool == 'power.metal.avg_dataTR0.2'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:220px',onclick="sortTable("+table1count+")") pwr.metal.AVG_dataTR0.2 >100%[M5-M16]
                        else if tool == 'power.via.avg_normal'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:220px',onclick="sortTable("+table1count+")") pwr.via.AVG_dataTR0.5 >100%[VIA0-VIA4]
