                        else if tool == 'sig.metal.avg_normal'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:120px',onclick="sortTable("+table1count+")") sig.metal.AVG > 100%
                        else if tool == 'sig.metal.peak_normal'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:130px',onclick="sortTable("+table1count+")") sig.metal.PEAK > 100%
                        else if tool == 'sig.metal.rms_normal'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:120px',onclick="sortTable("+table1count+")") sig.metal.rms > 100%
                        else if tool == 'sig.via.avg_normal'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:108px',onclick="sortTable("+table1count+")") sig.via.AVG > 100%
                        else if tool == 'sig.via.peak_normal'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:108px',onclick="sortTable("+table1count+")") sig.via.PEAK > 100%
                        else if tool == 'sig.via.rms_normal'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:108px',onclick="sortTable("+table1count+")") sig.via.rms > 100%
                        else if tool == 'sig.metal.peak.a0final_normal'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:120px',onclick="sortTable("+table1count+")") sig.metal.PEAK > 110%	
                        else if tool == 'power.metal.avg.worstcase_dataTR0.2'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:195px') pwr.metal.AVG_dataTR0.2.WC
                        else if tool == 'power.metal.avg.worstcase_normal'
