                        else if tool == 'drc_PWR_HU'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:110px') #{tool}
                        else if tool == 'sta_primetime_date'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:195px') sta_primetime_date
                        else if tool == 'pg_opens'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:120px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'stair'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:120px',onclick="sortTable("+table1count+")") StaIR => 4.5%
                        else if tool == 'stair_worstcase'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:120px',onclick="sortTable("+table1count+")") StaIR_WC
                        else if tool == 'dvd'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:120px',onclick="sortTable("+table1count+")") DynIR 
